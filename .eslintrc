{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "eol-last": "off",
    "padded-blocks": "off",
    "linebreak-style": "off",
    "brace-style": "off",
    "operator-linebreak": "off"
  }

}